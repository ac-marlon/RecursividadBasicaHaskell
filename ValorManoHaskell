calcular::[(String,String)] -> Int
calcular [] = 0
calcular ((xv,xp):xs) = if valor_carta xv == 1 && calcular xs <=10 then 11 + calcular xs
			else valor_carta xv + calcular xs

valor_carta:: String -> Int
valor_carta x = 
	if x == "J" then 10
	else if x == "Q" then 10
	else if x == "K" then 10
	else if x == "A" then 1
	else (read x :: Int) 
